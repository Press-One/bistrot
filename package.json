{
    "name": "prs-atm",
    "description": "A CLI tool for financing on PRESS.one.",
    "version": "2.0.9",
    "private": false,
    "homepage": "https://github.com/Press-One/prs-atm",
    "main": "index.js",
    "bin": {
        "prs-atm": "./bin/prs-atm.js"
    },
    "scripts": {
        "start": "node index.js",
        "debug": "node --inspect index.js",
        "readme": "cat template.md > README.md && echo '\n```' >> README.md && ./bin/prs-atm.js >> README.md && echo '```' >> README.md",
        "docker": "docker build --no-cache . -t pressone/prs-atm && docker push pressone/prs-atm",
        "docker-cn": "docker build --no-cache . -t dockerhub.qingcloud.com/pressone/prs-atm && docker push dockerhub.qingcloud.com/pressone/prs-atm",
        "prepublishOnly": "npm version patch && npm update && ( git commit -am 'npm update' || true ) && git pull && git push"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Press-One/prs-atm.git"
    },
    "dependencies": {
        "eosjs-keygen": "^1.3.2",
        "keythereum-pure-js": "^1.1.8",
        "readline-sync": "^1.4.10",
        "sushitrain": "^1989.6.88",
        "table": "^5.4.6",
        "utilitas": "^1989.6.47",
        "uuid": "^3.4.0",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "@nodeutils/defaults-deep": "^1.1.0",
        "aes-js": "^3.1.2",
        "bignumber.js": "^9.0.0",
        "cids": "^0.8.2",
        "eosjs-ecc": "^4.0.7",
        "ipfs": "^0.43.0",
        "ipfs-bitswap": "^0.29.1",
        "ipfs-repo": "^2.0.1",
        "libp2p": "^0.27.7",
        "libp2p-kad-dht": "^0.18.7",
        "libp2p-mplex": "^0.9.5",
        "libp2p-secio": "^0.12.4",
        "libp2p-tcp": "^0.14.5",
        "mathjs": "^6.6.5",
        "mime-types": "^2.1.27",
        "multihashing": "^0.3.3",
        "ncp": "^2.0.0",
        "peer-id": "^0.13.12",
        "peer-info": "^0.17.5",
        "promisify-es6": "^1.0.3",
        "rimraf": "^3.0.2"
    }
